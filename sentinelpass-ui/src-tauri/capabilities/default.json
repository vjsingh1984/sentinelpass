{
  "identifier": "default",
  "description": "Default capabilities for the main window",
  "windows": ["main"],
  "permissions": [
    "clipboard-manager:allow-write-text",
    "clipboard-manager:allow-read-text",
    "clipboard-manager:allow-clear",
    "shell:allow-open",
    "dialog:allow-ask",
    "dialog:allow-confirm",
    "dialog:allow-message",
    "dialog:allow-open",
    "dialog:allow-save"
  ]
}
